				<last_execute_cmd/>
			</config>
			<agent_client>
				<server_port>51510</server_port>
				<server_addr>127.0.0.1</server_addr>
				<local_proofd_port>1093</local_proofd_port>
			</agent_client>
		</instance>
		<instance name="client1" type="client">
			<config>
				<work_dir>/tmp/</work_dir>
