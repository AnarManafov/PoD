<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<proofagent_config last_update="2008-01-15" version="1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www-linux.gsi.de/~manafov/D-Grid/docz/proofagent.cfg.xsd">
	<instances>
		<instance name="server" type="server">
			<config>
				<work_dir>/tmp/</work_dir>
				<logfile_dir>/tmp/</logfile_dir>
				<logfile_overwrite>true</logfile_overwrite>
				<log_level>0</log_level>
				<timeout>0</timeout>
				<proof_cfg_path>~/proof.conf</proof_cfg_path>
				<last_execute_cmd/>
			</config>
			<agent_server>
				<listen_port>21111</listen_port>
				<local_client_port_min>20000</local_client_port_min>
				<local_client_port_max>25000</local_client_port_max>
			</agent_server>
		</instance>
		<instance name="client" type="client">
			<config>
				<work_dir>/tmp/</work_dir>
				<logfile_dir>_G_WRK_DIR</logfile_dir>
				<logfile_overwrite>true</logfile_overwrite>
				<log_level>0</log_level>
				<timeout>0</timeout>
				<proof_cfg_path>_G_WRK_DIR/proof.conf</proof_cfg_path>
				<last_execute_cmd/>
			</config>
			<agent_client>
				<server_port>21111</server_port>
				<server_addr>depc218.gsi.de</server_addr>
				<local_proofd_port>1093</local_proofd_port>
			</agent_client>
		</instance>
	</instances>
</proofagent_config>
